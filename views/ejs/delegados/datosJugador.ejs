<!doctype html>
<html class="no-js" lang="">
  <head>
    <meta charset="utf-8">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Torneo de Fútbol Para Socios del Club Atlético San Lorenzo de Almagro</title>
    <link rel="shortcut icon" href="favicon.ico">
    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
     <link rel="stylesheet" href="vendor.css">       
     <link rel="stylesheet" href="style.css">   
     <link rel="stylesheet" type="text/css" href="css/layerslider.css">
    <script src="js/vendor/modernizr.js"></script>    
  </head>
  <body>
    <!--[if lt IE 10]>
      <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->
    <div class="wrapper">
      <!-- header -->
      <header class="header-main">
        <div class="header-lower clearfix">
      <%- include ../menu.ejs %>
        </div>  
</header>

       <!-- banner section -->
      <div class="innerbannerwrap">
        <div class="content"></div>
        <div class="innerbannergoleadores">
          <h2 class="bannerHeadline">Datos del jugador <%= jugador.nombre %> <%= jugador.apellido %> <span></span></h2>
        </div>
      </div>

      <% if (resultado) { %>
          <% if (resultado == 200) { %>
                <div class="alert alert-success">La operacion fue realizada con exito</div>
          <% } else { %>
              <div class="alert alert-danger">La operacion fallo. Inténtelo nuevamente más tarde</div>
          <% } %>
      <% } %>

      <section class="innerpage_all_wrap bg-white"> 
        <div class="container">
			<div class="row">
			<div class="matchSchedule_details row">
          
				      		<div class="match_versus-wrap2">
			                  <div class="wrap_match-innerdetails">
			                    <ul class="point_table2">
			                       <li class="clearfix">
			                        <div class="subPoint_table">
			                          <div class="headline01 smallpoint">Nombre</div>
			                          <div class="headline01 smallpoint">Apodo</div>
			                          <div class="headline01 smallpoint">Fecha de nacimiento</div>
			                          <div class="headline01 smallpoint">DNI</div>
			                          <div class="headline01 smallpoint">Posicion</div>
			                          <div class="headline01 smallpoint">Numero</div>
			                          <div class="headline01 smallpoint">Email</div>
                                	  <div class="headline01 smallpoint">Capitan</div>
                                	  <div class="headline01 smallpoint">SubCapitan</div>
                                    </div>
            						</div>
                      				</li>
            					     <li class="clearfix">
                                     			 <div class="subPoint_table">
                                     			 		<div class="headline01 smallpoint"><%= jugador.nombre %> <%= jugador.apellido %></div>
                                            			 <div class="headline01 smallpoint"><%= jugador.apodo %></div>
                                            			 <div class="headline01 smallpoint"><%= moment(jugador.fecha_de_nacimiento).format('DD/MM/YYYY') %></div>
                                            			 <div class="headline01 smallpoint"><%= jugador.dni %></div>
                                            			 <div class="headline01 smallpoint"><%= jugador.posicion %></div>
                                            			 <div class="headline01 smallpoint"><%= jugador.numero %></div>
                                            			 <div class="headline01 smallpoint"><%= jugador.email %></div>

                                                   <div class="headline01 smallpoint">
                                                      <% if ( equipo.capitan == jugador._id ) { %>
                                                        SI
                                                      <% } else { %> NO <% } %>
                                                    </div>
                                                  <div class="headline01 smallpoint">
                                                      <% if ( equipo.subcapitan == jugador._id ) { %>
                                                        SI
                                                      <% } else { %> NO <% } %>
                                                    </div>
                                            			
                      				</li>
                    </ul>
                    </div>
                  </div>
 				</div>
            </div>
        </div>
				 
		
		<form action="/delegado" method="get">
             <button type="submit">VOLVER A MI EQUIPO</button>
        </form>
      </section>
    

      <!-- footer -->
      <footer ><!--class="footer-type01"-->
	   <%- include ../footer.ejs %>
      </footer>
    </div>
    

      
    <script src="js/vendor/vendor.js"></script>     
    <script src="js/main.js"></script>
	<script src="js/admin.js"></script> 
    <script src="js/partidos.js"></script> 
</body>
</html>
